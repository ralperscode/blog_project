<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" id="title" type="text" name="postTitle">
    <label for="body">Post</label>
    <textarea class="form-control" id="body" type="text" name="postBody" rows="3"></textarea>
  </div>
  <button class= "btn btn-primary" type="submit" name="composeButton">Publish</button>
</form>

<%- include("partials/footer") -%>
